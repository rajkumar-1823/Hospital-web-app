{
    "version":2,
    "builds": [
      { 
        "src": "*.js", 
        "use": "@vercel/node" 
    }
    ],
        "routes": [
          { 
            "src": "/(.*)", 
            "dest": "/" ,
            "methods": ["POST","GET"],
            "headers": { 
              "Access-Control-Allow-Origin": "https://hospital-web-app.vercel.app",
              "Access-Control-Allow-Credentials":"true"
            }
        }
        ]
      
  }